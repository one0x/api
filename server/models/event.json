{
  "name": "event",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "transactionHash": {
      "type": "string",
      "id": true
    },
    "transactionIndex": {
      "type": "number"
    },
    "logIndex": {
      "type": "number"
    },
    "blockNumber": {
      "type": "number"
    },
    "result": {
      "type": "string"
    },
    "error": false,
    "createdAt": false,
    "updatedAt": false
  },
  "validations": [],
  "relations": {
    "peer": {
      "type": "hasMany",
      "model": "protocol_peer",
      "foreignKey": "sourceId",
      "through": "event_has_peer",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
